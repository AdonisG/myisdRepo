class Book():
    def __init__(self, title, author, publicationyear):
        self.title=title
        self.author=author
        self.publicationyear=publicationyear
        
    def showDetails(self):
        print("Title :", self.title)
        print("Author :",self.author)
        print("Year of Publication :",self.publicationyear)

    def setTitle(self, inputTitle):
        self.title=inputTitle

    def getTitle(self):
        return self.title
        
        
        
Book1= Book("Game of Thrones","George R. R. Martin", 2000)
Book1.showDetails()
Book1.setTitle("abc")
Book1.showDetails()
print(Book1.getTitle())
